#include<stdio.h>
#include<stdlib.h>

void main()
{
	printf("Programe to measure the area of a rectangle\n");
	int length, breadth, a;

	printf("Enter the length of the rectangle :\n");
		scanf("%d",&length);
	printf("Enter the breadth of the rectangle :\n");
		scanf("%d",&breadth);

	a = length * breadth;

	printf("Area of the rectangle is %d \n",a);
}	
			
