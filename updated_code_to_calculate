#include<stdio.h>
#include<stdlib.h>

void main()
{
	   printf("Programe for calculating time\n\n");

	float time, speed, distance;

	  printf("\n Enter speed mph : ");
	  	scanf("%f",&speed);

	  printf("\n Enter distance in miles:");
		  scanf("%f",&distance);

	  time = distance/speed;	

	  printf("\n Travelled at the speed of %f and covered %f miles in %f hours \n\n",speed ,distance ,time);
}	  
